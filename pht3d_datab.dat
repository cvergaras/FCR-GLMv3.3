#################################################	  
Chlorite(14A) # Lowson et al. (2005)
#################################################	 
 -start
   15 if (m<=1e-12 and SR("Chlorite(14A)")<=1) then goto 200
   16 sat_ratio = SR("Chlorite(14A)") 
   20 rate_H = 1.622e-10* ACT("H+")^0.49 							
   30 rate_nb = 1e-13 + 1.622e-17/(ACT("H+")^0.43)  						
   40 mzero=m0
   45 if m0 = 0  then mzero=1
   60 rate = parm(1)*(rate_H + rate_nb)*(1-sat_ratio)*(m/mzero)^0.67
   70 moles = rate * time 
   100 if (moles > m) then moles = m
   200 save moles
 -end


