#################################################
Orgc_sed 
#################################################
# Appelo and Postma Rate, including temperature dependence 
# as detailed in Greskowiak et al. 2006 and Sharma et al. 2012
#################################################
-start
   #1 goto 200
   2 mOrgc_sed = TOT("Orgc_sed") * 1000
   3 m_zero = 0.000456*1000 #7.603E-01  #
   4 if (mOrgc_sed <= 1e-9) then goto 200
	# Temperature dependence
  20  mTmp   = tot("Tmp")*1000 *1000             			# Tmp entered, e.g. 2.46e-5 C due to low IS Ro water	
  21  mTopt  = 35
  22  par1=  -1.50							#Sharma et al. 2012; Greskowiak et al. 2006
  23  par2=  0.18							#Sharma et al. 2012; Greskowiak et al. 2006
  25  fT = exp(par1+ par2*mTmp*(1-0.5*(mTmp/mTopt)))
  26 put(fT, 89*50+CELL_NO)
	#
  30  mO2  = mol("O2")
  31  mNO3 = TOT("N(5)")  
	#25  mSO4 = tot("S(6)")
	#30   mFe3 = EQUI("FeOH(a)") 					#KIN("Goethite")
	#35  redFe = 9e-13*mFe3/(1.e-5 + mFe3) 				#6e-12*mFe3/(1.e-5 + mFe3)   
  40  rO2 = parm(1) * mO2/(2.94e-4 + mO2)		#(1.e-5 + mO2)   	#parm(1) = 1.57e-9  in phreeqc datab; 2.5e-9 in Carlos model
  41 put(rO2, 90*50+CELL_NO)
  50 rNO3 = parm(2) * mNO3/(1.55e-4 + mNO3)		#(1.e-5 + mNO3) 	#parm(2) = 1.67e-11 in phreeqc datab; 1.e-11 in Carlos model
  51 put(rNO3, 91*50+CELL_NO)
	60 rSO4 = parm(3)* mSO4/(1.e-4 + mSO4)    			#parm(3) = 1.e-13   in phreeqc datab; 1.e-13 in Carlos model
   61 put(rSO4, 92*50+CELL_NO)
   70  rate = rO2 + rNO3                                        			#previously in Carlos db: "... #+ redFe #+ redMn2"
	71 put(rate, 93*50+CELL_NO)
#	75  moles = rate * mOrgc_sed *(mOrgc_sed/m_zero) * time 	#previously in Carlos db : " moles = (rate) * time *(mOrgc_sed/m_zero)^0.67"
  75  moles = fT * rate * time # * (mOrgc_sed/m_zero)^0.67  
  200 save moles
-end
  
#################################################
Calcite  # phreeqc DB expression
#################################################
## Example of KINETICS data block for calcite rate:
##	KINETICS 1
##	Calcite
##		-tol    1e-8
##		-m0 	3.e-3
##		-m	3.e-3
##		-parms	50  	0.6
##   rem  parm(1) = A/V, 1/dm	  parm(2) = exponent for m/m0

-start
   #1  goto 200
    15  si_cc = si("Calcite")
    20  if (m <= 1e-12  and si_cc < 0) then goto 200
    21  tempC   = tot("Tmp")*1000 *1000            			# temp entered, e.g. 2.46e-5 due to low IS Ro water
    30  k1 = 10^(0.198 - 444.0 / (273.16 + tempC)) 			#+ tc) )
    40  k2 = 10^(2.84 - 2177.0 / (273.16 + tempC)) 			#+ tc) )
	#50  k3 = 10^(-5.86 - 317.0 / (273.16 +25))				#+ tc) )
    50  if tempC <= 25 then k3 = 10^(-5.86 - 317.0 / (273.16 + tempC)) 		#+ tc) )
    60  if tempC >  25 then k3 = 10^(-1.1 - 1737.0 / (273.16 + tempC)) 		#+ tc) )
    70  t = 1
    80  if m0 > 0 then t = m/m0
    90  if t = 0 then t = 1
   100  moles = parm(1) * 0.1 * (t)^parm(2)
   110  moles = moles * (k1 * act("H+") + k2 * act("CO2") + k3 * act("H2O"))
   120  moles = moles * (1 - 10^(2/3*si_cc))
   130  moles = moles * time 
   140  if (moles >  m) then moles = m
	  #150  if (moles >= 0) then goto 200
	  #160  temp = tot("Ca")
	  #170  mc  = tot("C(4)")
	  #180  if mc < temp then temp = mc
	  #190  if -moles > temp then moles = -temp 
	  #191 rate = moles/time
   200 save moles
-end

################################################################
Pyrite
#################################################################
## rate equation Williamson & Rimstidt 1994, GCA 58
## parameters according to Appelo et al. 1998 , Appl. Geochem. 13
## using correct. factor FeS2 -> FeS and O2 -> NO3
## parm(1) = nitrate(before: log10(A/V, 1/dm)) parm(2) = exp for (m/m0);
## parm(3) = exp for O2 parm(4) = exp for H+
## parm(5) = surface over volume factor: log10(mPyr*parm(5)); replaces parm(1)

-start 
   19 mTmp   = tot("Tmp")*1000 *1000             			# Tmp entered, e.g. 2.46e-5 C due to low IS Ro water
   20 si_pyr = SI("Pyrite")
   21 sr_pyr = SR("Pyrite")
   22 if (m <= 1e-12 and sr_pyr < 1) then goto 200
	# Temperature dependence
   23   par1 = -6758.1
   24   par2 =    16.1
   25   invtemp       = 1/(mTmp+273.15)					
   26   invtemp25     = 1/(25+273.15)            			#Arrh-factor set to 1 at 25C, this is a scaling factor only
   27   y25           = -exp(invtemp25*par1+par2)
   28   yTmp          = -exp(invtemp*par1+par2)
   29   arrh_factor   = yTmp/y25
   #
   30   rate_oxy      = (mol("O2"))^parm(3)   * (mol("H+"))^parm(4)
   
   40   rate_nitr     = (mol("NO3-"))^parm(3) * (mol("H+"))^parm(4)
   
   43   mPyr          = KIN("Pyrite")
   45   surf_over_vol = log10(mPyr*parm(5))               		#parm(5)= 115 = value used in Prommer & Stuyfzand, 2005 
   #
   52   f1 = 10^(-10.19 + surf_over_vol) * (m/m0)^parm(2)
   53   r_Oxy = f1 * rate_oxy * arrh_factor 
   60   moles_oxy = r_Oxy * time
	# limit O2 oxidation to maximal stoichiometry for FeS -> Fe_tri+3 + SO4
   62   m_o2 = mol("O2") * 4/9						#should it not be ("O(0)")?
   64   if moles_oxy > m_o2 then moles_oxy = m_o2
   #
   69   f2 = parm(1)   #0.01						# Factor f2 was assumed to equal unity in Eckert and Appelo 2002 as well as Prommer and Stuyfzand 2005
   70   r_nit = f2 * f1 * rate_nitr * arrh_factor
   80   moles_nitr = r_nit * TIME 
        #82   moles_0 = f1 * rate_0 * arrh_factor * TIME		#what is rate_0, hydrolysis?
   90   moles = moles_oxy + moles_nitr    			#+ moles_0 
   100  if (moles > m) then moles = m
 #  150  put(moles,40)
   200  save moles
   201 put(r_oxy, 101*50+CELL_NO)
   202 put(r_nit, 102*50+CELL_NO)
   203 put(moles, 40*50+CELL_NO)
   -end

#####################################
K-feldspar # Last edited May 2012
#####################################
# Rate expression based on previous phreeqc DB rate expression and 
# Sverdrup(1990) & Sverdrup and Warvinger (1995) in Appelo and Postma (2000)(Equation 8.26)
# re-precipitation was reduced from x 0.1 to x 0.001 as feldspar dissolution assumed irreversible for T<50C (Appelo & Postma 2000, p.384

-start
#1 rem specific rate in kmol/m2/s
#2 rem parm(1) = (A/V) *10 in (1/dm), (unit conversion A (m2) and V(dm3) results in *10 (1/dm)); (recalc sp. rate to mol/kgw)
#3 rem Second correction factor for field rate relative to lab rate needed ? At the moment included in A over V expression.
#4 rem temp corr: E (kJ/mol) / R / 2.303 = H in H*(1/T-1/298) 
#5 rem given Ea/(R*2.303): pkH=3500 , pkH2O=2000 , pkOH=2500 , pkCO2 = 2000 , pKOrg=3000 
 
#1 goto 100
 2 if (m<=1e-12) then goto 100
      # Activities of inhibiting ions
12    a_Al =act("Al+3")
13    BC = act("Na+") + act("K+") + act("Mg+2") + act("Ca+2")
      #14   R = act("Organic_Acid_Radicals")                  	# need to be defined in DB!! 
      # Temperature correction of rates to 8C
16    tempK= 273.15 + tot("Tmp")*1000*1000                	        # Tmp entered, e.g. 2.46e-5 C due to low IS Ro water
17    dif_temp = 1/tempK - 1/281

20    pk_H = 11.7 + 3500 * dif_temp
30    pk_w = 14.5 + 2000 * dif_temp
40    pk_OH = 13.1 + 2500 * dif_temp
50    pk_CO2 = 13 + 2000 * dif_temp
      #60    pk_org = 12 + 3000 * dif_temp                     	# rate increase with DOC
      # Rate by H+, Hydrolysis, OH-, CO2 and org.acids (if applicable)
70    rate_H = 10^-pk_H * ACT("H+")^0.5  / ((1 + a_Al/4e-6)^0.4 *(1+BC/5e-4)^0.15)
72    rate_w = 10^-pk_w/((1 + a_Al/4e-6)^0.14 *(1+BC/5e-4)^0.15)
74    rate_OH = 10^-pk_OH * ACT("OH-")^0.3
76    rate_CO2 = 10^-pk_CO2 * (10^SI("CO2(g)"))^0.6
      #78   rate_org = 10^-pk_org * TOT("DOC")^0.4 / (1 + R/5e-6)^0.5
      # Calculate moles
80   if SR("K-feldspar") > 1 then goto 100
82    AoV = parm(1)
84    rate = rate_H + rate_w + rate_OH + rate_CO2
86    moles = AoV * rate * (1 - SR("K-feldspar")) * time *(m/m0)^0.67 
      #88    PUT(rate,8)
      # Decrease rate on precipitation
90    if SR("K-feldspar") > 1 then moles = moles * 0.001 
100   save moles
-end


