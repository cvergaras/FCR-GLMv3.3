####################################################
Ankerite   # Ankerite = Solid Solution with dolomite
####################################################
#Rate for dolomite from Chou et al 1989. mol/cm^2/s
-start
	#1  goto 200
	#2 Fe=EQUI("Fe(OH)3(a)")
	#3 k_fe=1e-1
	#4 if Fe > k_fe then Fe = k_fe
	#2 if si("Calcite")<0 then goto 200
 5 if (m<= 1e-12) then goto 200
10 k1 = 2.6e-7
20 k2 = 1e-8 		
30 k3 = 2.2e-12 	
	#40   t = 1
	#50   if m0 > 0 then t = m/m0
	#60   if t = 0 then t = 1
70   f1 = parm(1) *(m/m0)^0.67       					#* (t)^0.67  
80 rate = f1 * (k1 * act("H+")^0.75 + k2 * act("CO2")^0.75 + k3 ) 	# Chou: n=0.75; Busenberg and Plummer: n=0.5; Carlos: n=0.65
90 moles = rate * time						        #*(k_fe - Fe)/k_fe
	#95  PUT(rate, 2)
100  if (moles > m) then moles = m
	#110 if (moles >= 0) then goto 200
200 save moles
-end

######################################
Siderite
######################################
# Rate based on: Duckworth & Martin (2004),Geochim.Cosmochim.Acta 
# Rate in [mol/m2*s] 
# During experiment passivation of siderite through ferihydrite precipitation was observed
# Therefor a term for passivation was included assuming an exponential relationship as observed by Jeen et al.(2007),EST for the passivation of zero-valent iron in PRB
# Anox. rate equation valid for pH>3 and T=298K; pH range tested: 1.5 < pH < 12
# Ox. rate equation not verified by publication, Duckworth& Martin observed rate reduction below LOD

-start
#1   rem	parm(1) = A/V, 1/dm 	parm(2) = fiting parameter for exp(par(2)* [ferihydrite]) needs to be high to promote passivation
1    if (m  <= 1e-12) then goto 200
20   if (SR("Siderite") > 1) then goto 200
30   k1 = 10^(-4.6)
40   k2 = 10^(-8.65)
#50  r_anox = parm(1) * (k1 * act("H+")^0.95) * (1 - SR("Siderite")) * (m/m0)^0.67
50   r_anox = parm(1) * (k1 * act("H+")^0.75 + k2 * act("H2O")) * (1 - SR("Siderite")) * (m/m0)^0.67
60   fh = EQUI("Fe(OH)3(a)")		#KIN("Fe(OH)3(a)")
70   r_ox = r_anox * exp(-parm(2) * fh)
75   if (fh <= 0) then r_ox = 0
100  moles = r_ox * time
110  if (moles > m) then moles = m
200  save moles
-end


#################################################
Fe_di  # from Eckert and Appelo 2002
#################################################
# in E&A paper: par(1)=2.91e-9; par(2)=1.33e12; par(3)=773; par(4)=200 
# in Wallis et al 2010: moles = par(2)*(ACT("OH-"))^2*(p_o2 + mNO3); par(1)=0, par(3)=par(4)=1
-start
10  Fe_di = TOT("Fe_di")
20  if (Fe_di <= 1e-12) then goto 200
30  p_o2 = 10^(SI("O2(g)"))
35  mNO3 = mol("NO3-")
36 rate = parm(1) + parm(2)*(ACT("OH-"))^2*(parm(3)*p_o2 + parm(4)*mNO3)
37 put(rate, 200*50+CELL_NO)
40  moles = rate * Fe_di * TIME
50  if (moles > m) then moles = m
200 SAVE moles
-end

